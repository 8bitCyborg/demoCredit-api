{
  "name": "democredit",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "node --env-file=.env --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "migrate:make": "NODE_OPTIONS='--loader ts-node/esm' npx knex migrate:make --knexfile knexfile.ts",
    "migrate:latest": "NODE_OPTIONS='--loader ts-node/esm --no-warnings' knex migrate:latest",
    "migrate:rollback": "NODE_OPTIONS='--loader ts-node/esm --no-warnings' knex migrate:rollback",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/knex": "^0.15.2",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.3.1",
    "knex": "^3.1.0",
    "mysql2": "^3.18.0",
    "rate-limiter-flexible": "^9.1.1",
    "zod": "^4.3.6"
  }
}
